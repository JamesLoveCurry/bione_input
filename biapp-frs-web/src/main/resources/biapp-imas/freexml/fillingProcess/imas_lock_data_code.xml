<?xml version="1.0" encoding="utf-8"?>
<root>

    <templet>
        <templetname>数据锁定</templetname>
        <fromtable>imas_lock_data</fromtable>
        <savetable>imas_lock_data</savetable>
        <pkname>rid</pkname>
        <autoquery>N</autoquery>
        <ishavebillquery>Y</ishavebillquery>
        <list_btns></list_btns>
        <card_size>900*560</card_size>
        <orderbys>data_dt desc</orderbys>
        <list_ispagebar>Y</list_ispagebar>
        <list_ischeckstyle>Y</list_ischeckstyle>
        <list_ismultisel>Y</list_ismultisel>
    </templet>

    <templet_item>
        <itemkey>rid</itemkey>
        <itemname>主键</itemname>
        <itemtype>文本框</itemtype>
        <defaultformula>UUID</defaultformula>
        <itemdefine></itemdefine>
        <issave>Y</issave>
        <ismust>N</ismust>
        <card_isshow>N</card_isshow>
        <card_width>180,370</card_width>
        <card_iswrap>Y</card_iswrap>
        <list_isshow>N</list_isshow>
        <list_width>100</list_width>
        <list_align>中</list_align>
        <query_isshow>N</query_isshow>
        <query_width>200</query_width>
    </templet_item>
    
    <templet_item>
		<itemkey>org_name</itemkey>
		<itemname>机构名称</itemname>
		<itemtype>文本框</itemtype>
		<card_isshow>Y</card_isshow>
		<card_width>250</card_width>
		<card_iswrap>N</card_iswrap>
		<list_isshow>Y</list_isshow>
		<list_width>150</list_width>
		<list_align>中</list_align>
		<query_isshow>N</query_isshow>
		<query_itemtype>自定义参照</query_itemtype>
		<query_comparetype>in</query_comparetype>
		<query_itemdefine>【jsfile=/yujs/imas/report/imas_report_orgfilter_dialog.js】【cols=org_no->org_no】【winsize=600*560】</query_itemdefine>
		<checkbox>Y</checkbox>
		<tree_ischeckboxcascade>Y</tree_ischeckboxcascade>
		<query_width>300</query_width>
	</templet_item>
    
    <templet_item>
		<itemkey>org_no</itemkey>
		<itemname>机构名称</itemname>
		<itemtype>自定义参照</itemtype>
		<comparetype>in</comparetype>
		<itemdefine>【jsfile=/yujs/imas/report/imas_report_orgfilter_dialog.js】【cols=org_no->org_no】【winsize=600*560】</itemdefine>
		<checkbox>Y</checkbox>
		<tree_ischeckboxcascade>Y</tree_ischeckboxcascade>
		<card_isshow>Y</card_isshow>
		<card_width>250</card_width>
		<card_iswrap>N</card_iswrap>
		<list_isshow>N</list_isshow>
		<list_width>150</list_width>
		<list_align>中</list_align>
		<query_isshow>Y</query_isshow>
		<query_itemtype>自定义参照</query_itemtype>
		<query_comparetype>in</query_comparetype>
		<query_itemdefine>【jsfile=/yujs/imas/report/imas_report_orgfilter_dialog.js】【cols=org_no->org_no】【winsize=600*560】</query_itemdefine>
		<checkbox>Y</checkbox>
		<tree_ischeckboxcascade>Y</tree_ischeckboxcascade>
		<query_width>300</query_width>
	</templet_item>

    <templet_item>
        <itemkey>tab_name</itemkey>
        <itemname>表名</itemname>
        <itemtype>自定义参照</itemtype>
		<helptip></helptip>
		<itemdefine>【jsfile=/yujs/imas/common/imas_tab_dialog.js】【cols=tab_name->tab_names;】【winsize=600*540】</itemdefine>
        <issave>Y</issave>
        <ismust>N</ismust>
        <card_isshow>Y</card_isshow>
        <card_width>180,370</card_width>
        <card_iswrap>Y</card_iswrap>
        <list_isshow>Y</list_isshow>
        <list_width>150</list_width>
        <list_align>中</list_align>
        <query_isshow>Y</query_isshow>
        <query_itemtype>下拉框</query_itemtype>
        <query_itemdefine>【{"类型":"SQL","内容":"select tab_name id,tab_name name from imas_cr_tab where tab_belong = '业务表' order by tab_no"}】</query_itemdefine>
        <query_width>280</query_width>
    </templet_item>
    
    <templet_item>
        <itemkey>data_dt</itemkey>
        <itemname>数据日期</itemname>
        <itemtype>日历</itemtype>
        <itemdefine></itemdefine>
        <issave>Y</issave>
        <ismust>N</ismust>
        <isonlyone>Y</isonlyone>
        <card_isshow>Y</card_isshow>
        <card_width>180,370</card_width>
        <card_iswrap>Y</card_iswrap>
        <list_isshow>Y</list_isshow>
        <list_width>150</list_width>
        <list_align>中</list_align>
        <query_iswrap>Y</query_iswrap>
        <query_isshow>Y</query_isshow>
        <query_width>300</query_width>
    </templet_item>

    <templet_item>
        <itemkey>status</itemkey>
        <itemname>数据状态</itemname>
        <itemtype>下拉框</itemtype>
        <itemdefine>{"类型":"直接值","内容":"1/全部锁定|2/维护加锁|3/填报加锁|4/全部解锁"}</itemdefine>
        <issave>Y</issave>
        <ismust>N</ismust>
        <card_isshow>Y</card_isshow>
        <card_width>180,370</card_width>
        <card_iswrap>Y</card_iswrap>
        <list_isshow>Y</list_isshow>
        <list_width>150</list_width>
        <list_align>中</list_align>
        <query_isshow>Y</query_isshow>
        <query_width>280</query_width>
    </templet_item>

    <templet_item>
        <itemkey>operator</itemkey>
        <itemname>操作人</itemname>
        <itemtype>文本框</itemtype>
        <itemdefine></itemdefine>
        <issave>Y</issave>
        <ismust>N</ismust>
        <card_isshow>Y</card_isshow>
        <card_width>180,370</card_width>
        <card_iswrap>Y</card_iswrap>
        <list_isshow>Y</list_isshow>
        <list_width>150</list_width>
        <list_align>中</list_align>
        <query_isshow>N</query_isshow>
        <query_width>200</query_width>
    </templet_item>

    <templet_item>
        <itemkey>operator_name</itemkey>
        <itemname>操作人名称</itemname>
        <itemtype>文本框</itemtype>
        <itemdefine></itemdefine>
        <issave>Y</issave>
        <ismust>N</ismust>
        <card_isshow>Y</card_isshow>
        <card_width>180,370</card_width>
        <card_iswrap>Y</card_iswrap>
        <list_isshow>Y</list_isshow>
        <list_width>150</list_width>
        <list_align>左</list_align>
        <query_isshow>N</query_isshow>
        <query_width>200</query_width>
    </templet_item>

    <templet_item>
        <itemkey>create_tm</itemkey>
        <itemname>操作时间</itemname>
        <itemtype>文本框</itemtype>
        <itemdefine></itemdefine>
        <issave>Y</issave>
        <ismust>N</ismust>
        <card_isshow>Y</card_isshow>
        <card_width>180,370</card_width>
        <card_iswrap>Y</card_iswrap>
        <list_isshow>Y</list_isshow>
        <list_width>200</list_width>
        <list_align>中</list_align>
        <query_isshow>N</query_isshow>
        <query_width>200</query_width>
    </templet_item>

</root>
