<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yusys.cr.icrotherinf.repository.IcrInOtherBaseDao">
<!-- 根据条件查询报表列表 -->

	<select id="getInAcctEntDelList" parameterType="HashMap" resultType="com.yusys.cr.icrotherinf.entity.InAcctEntDel">
		select * from ICR_EntDel t where t.infRecType = #{infRecType} 
	</select>
	
	<select id="getInAcctDelList" parameterType="HashMap" resultType="com.yusys.cr.icrotherinf.entity.InAcctDel">
