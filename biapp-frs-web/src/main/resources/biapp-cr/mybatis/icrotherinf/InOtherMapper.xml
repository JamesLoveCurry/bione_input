		select * from ICR_InDel t where t.infRecType = #{infRecType} 
	</select>
	
	<select id="getInSecAcctIDCagsInfList" parameterType="HashMap" resultType="com.yusys.cr.icrotherinf.entity.InSecAcctIDCagsInf">
		select * from ICR_IdCagsInf t where t.infRecType = #{infRecType} 
	</select>
	
	
</mapper> 