<?xml version="1.0" encoding="utf-8"?>
<root>
    <templet>
        <templetname>选择报表</templetname>
        <ishavebillquery descr="是否有查询面板">N</ishavebillquery>
        <list_ispagebar>N</list_ispagebar>
        <list_ischeckstyle>N</list_ischeckstyle>
        <list_ismultisel>N</list_ismultisel>
    </templet>

    <templet_item>
		<itemkey>org_no</itemkey>
		<itemname>机构名称</itemname>
		<itemtype>自定义参照</itemtype>
		<comparetype>in</comparetype>
		<itemdefine>【jsfile=/yujs/bfd/common/bfd_report_orgfilter_dialog.js】【cols=org_no->org_no】【winsize=600*560】</itemdefine>
		<checkbox>Y</checkbox>
		<tree_ischeckboxcascade>Y</tree_ischeckboxcascade>
		<card_isshow>Y</card_isshow>
		<issave>Y</issave>
        <ismust>Y</ismust>
		<card_width>300</card_width>
		<card_iswrap>N</card_iswrap>
		<list_isshow>Y</list_isshow>
		<list_width>150</list_width>
		<list_align>中</list_align>
		<query_isshow>Y</query_isshow>
		<query_itemtype>自定义参照</query_itemtype>
		<query_comparetype>in</query_comparetype>
		<query_itemdefine>【jsfile=/yujs/bfd/common/bfd_report_orgfilter_dialog.js】【cols=org_no->org_no】【winsize=600*560】</query_itemdefine>
		<checkbox>Y</checkbox>
		<tree_ischeckboxcascade>Y</tree_ischeckboxcascade>
		<query_width>300</query_width>
	</templet_item>

    <templet_item>
        <itemkey>tab_name</itemkey>
        <itemname>表名</itemname>
        <itemtype>自定义参照</itemtype>
		<helptip></helptip>
		<itemdefine>【jsfile=/yujs/bfd/common/bfd_tab_dialog.js】【cols=tab_name->tab_name;】【winsize=600*540】</itemdefine>
        <issave>Y</issave>
        <ismust>Y</ismust>
        <card_isshow>Y</card_isshow>
        <card_width>300</card_width>
        <card_iswrap>Y</card_iswrap>
        <list_isshow>Y</list_isshow>
        <list_width>150</list_width>
        <list_align>中</list_align>
    </templet_item>

    <templet_item>
        <itemkey>data_dt</itemkey>
        <itemname>数据日期</itemname>
        <itemtype>日历</itemtype>
        <itemdefine></itemdefine>
        <ismust>Y</ismust>
        <card_isshow>Y</card_isshow>
        <card_width>300</card_width>
        <card_iswrap>Y</card_iswrap>
        <list_isshow>N</list_isshow>
        <list_width>100</list_width>
        <list_align>中</list_align>
    </templet_item>

    <templet_item>
        <itemkey>status</itemkey>
        <itemname>加锁类型</itemname>
        <itemtype>下拉框</itemtype>
        <itemdefine>{"类型":"直接值","内容":"1/全部加锁|2/维护加锁|3/填报加锁"}</itemdefine>
        <issave>Y</issave>
        <ismust>N</ismust>
        <card_isshow>Y</card_isshow>
        <card_width>300</card_width>
        <card_iswrap>Y</card_iswrap>
        <list_isshow>Y</list_isshow>
        <list_width>150</list_width>
        <list_align>中</list_align>
        <query_isshow>Y</query_isshow>
        <query_width>280</query_width>
    </templet_item>

</root>

