<?xml version="1.0" encoding="utf-8"?>
<root>
<!-- 该模板是平台使用的,构建灵活查询SQL界面上需要选择列 -->
<templet>
	<templetname>所有列</templetname>
	<ishavebillquery descr="是否有查询面板">N</ishavebillquery>
	<list_ispagebar>N</list_ispagebar>
</templet>

<templet_item>
    <itemkey>itemkey</itemkey>
    <itemname>itemkey</itemname>
    <itemtype>文本框</itemtype>
    <list_isshow>N</list_isshow>
    <list_width>100</list_width>
    <list_align>中</list_align>
</templet_item>


<templet_item>
    <itemkey>itemname</itemkey>
    <itemname>列名</itemname>
    <itemtype>文本框</itemtype>
    <list_isshow>Y</list_isshow>
    <list_width>160</list_width>
    <list_align>左</list_align>
</templet_item>

<templet_item>
    <itemkey>isquery</itemkey>
    <itemname>是否参与查询</itemname>
    <itemtype>勾选框</itemtype>
    <list_isshow>Y</list_isshow>
    <list_width>100</list_width>
    <list_checkaction>onChooseOneColumn</list_checkaction>
    <list_align>中</list_align>
</templet_item>

</root>
