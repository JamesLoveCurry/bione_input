<?xml version="1.0" encoding="utf-8"?>
<root>
	<templet>
		<templetname>数据处理日志</templetname>
		<fromtable>cams_data_log</fromtable>
		<savetable>cams_data_log</savetable>
		<pkname>rid</pkname>
		<querycontion></querycontion>
		<autocondition descr="自动查询时的条件"></autocondition>
		<autoquery descr="是否自动查询数据">Y</autoquery>
		<ishavebillquery descr="是否有查询面板">Y</ishavebillquery>
		<orderbys>data_time desc</orderbys>
		<card_btns></card_btns>
		<card_size>800*560</card_size>
		<card_isdebugborder>N</card_isdebugborder>
		<list_btns></list_btns>
		<list_issearch></list_issearch>
		<list_ispagebar>Y</list_ispagebar>
		<list_ischeckstyle>N</list_ischeckstyle>
	</templet>

	<templet_item>
		<itemkey>rid</itemkey>
		<itemname>主键</itemname>
		<itemtype>文本框</itemtype>
		<itemdefine></itemdefine>
		<defaultformula>UUID</defaultformula>
		<issave>Y</issave>
		<ismust>Y</ismust>
		<seq>5</seq>
		<card_isshow>N</card_isshow>
		<card_width>180,370</card_width>
		<card_iswrap>N</card_iswrap>
		<list_isshow>N</list_isshow>
		<list_width>120</list_width>
	</templet_item>
	
	<templet_item>
		<itemkey>data_id</itemkey>
		<itemname>数据ID</itemname>
		<itemtype>文本框</itemtype>
		<issave>Y</issave>
		<ismust>Y</ismust>
		<seq>5</seq>
		<card_isshow>N</card_isshow>
		<card_width>180,370</card_width>
		<card_iswrap>Y</card_iswrap>
		<list_isshow>N</list_isshow>
		<list_width>180</list_width>
		<list_align>左</list_align>
		<list_render></list_render>
		<query_isshow>N</query_isshow>
	</templet_item>

	<templet_item>
		<itemkey>status</itemkey>
		<itemname>操作名称</itemname>
		<itemtype>下拉框</itemtype>
		<itemdefine>【{"类型":"直接值","内容":"1/提交|2/复核通过|3/审批通过|4/退回"}】</itemdefine>
		<issave>Y</issave>
		<seq>5</seq>
		<card_isshow>Y</card_isshow>
		<card_width>180,370</card_width>
		<card_iswrap>N</card_iswrap>
		<list_isshow>Y</list_isshow>
		<list_width>100</list_width>
		<list_align>左</list_align>
		<query_isshow>N</query_isshow>
	</templet_item>

	<templet_item>
		<itemkey>reason</itemkey>
		<itemname>操作原因</itemname>
		<itemtype>文本框</itemtype>
		<issave>Y</issave>
		<seq>5</seq>
		<card_isshow>Y</card_isshow>
		<card_width>180,370</card_width>
		<card_iswrap>Y</card_iswrap>
		<card_iseditable>N</card_iseditable>
		<list_isshow>Y</list_isshow>
		<list_width>300</list_width>
		<list_align>左</list_align>
		<query_isshow>N</query_isshow>
	</templet_item>

	<templet_item>
		<itemkey>user_no</itemkey>
		<itemname>操作人</itemname>
		<itemtype>文本框</itemtype>
		<issave>Y</issave>
		<seq>5</seq>
		<card_isshow>Y</card_isshow>
		<card_width>180,370</card_width>
		<card_iswrap>Y</card_iswrap>
		<card_iseditable>N</card_iseditable>
		<list_isshow>Y</list_isshow>
		<list_width>150</list_width>
		<list_align>左</list_align>
		<query_isshow>N</query_isshow>
	</templet_item>

	<templet_item>
		<itemkey>data_time</itemkey>
		<itemname>操作时间</itemname>
		<itemtype>文本框</itemtype>
		<issave>Y</issave>
		<card_isshow>Y</card_isshow>
		<card_width>180,370</card_width>
		<card_iswrap>N</card_iswrap>
		<list_isshow>Y</list_isshow>
		<list_width>150</list_width>
		<list_align>左</list_align>
	</templet_item>
	
</root>

